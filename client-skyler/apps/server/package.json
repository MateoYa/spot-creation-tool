{
  "name": "server",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc --build --clean",
    "watch:ts": "tsc -w --preserveWatchOutput",
    "dev:old": "npm run build:ts && concurrently -k -p \"{name}\" -n \"TSC,APP\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start:cli": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "dev": "ts-node-dev --rs --clear --debounce 500 ./src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.425.0",
    "@fastify/autoload": "^5.7.1",
    "@fastify/sensible": "^5.2.0",
    "@kripod/uuidv7": "^0.3.4",
    "aws-sdk": "^2.1471.0",
    "better-sqlite3": "^8.5.0",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "dotenv": "^16.3.1",
    "fastify": "^4.23.2",
    "fastify-plugin": "^4.5.1",
    "fastify-type-provider-zod": "^1.1.9",
    "kysely": "^0.26.1",
    "typeid-js": "^0.3.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.4",
    "@types/bunyan": "^1.8.8",
    "@types/bunyan-format": "^0.2.5",
    "@types/node": "^18.16.19",
    "@types/tap": "^15.0.8",
    "concurrently": "^7.6.0",
    "fastify-tsconfig": "^1.0.1",
    "tap": "^16.3.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
